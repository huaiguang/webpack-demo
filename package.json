{
  "name": "webpack-demo",
  "version": "2.0.1",
  "description": "common webpack configuration",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist/",
    "clean:all": "rm package-lock.json & rm yarn.lock & rm -rf dist/ & rm -rf node_modules/",
    "dev": "node ./config/dev-server",
    "dev:all": "webpack --config ./config/webpack.conf.all",
    "dev:server": "webpack-dev-server --inline --hot --config ./config/webpack.conf.all",
    "build": "node ./config/build"
  },
  "keywords": [
    "webpack",
    "es6",
    "postcss",
    "eslint"
  ],
  "author": "Carlos <1483655267@qq.com>",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:huaiguang/webpack-demo.git"
  },
  "license": "MIT",
