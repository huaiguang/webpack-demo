    "babel-core": "^6.25.0",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.0.5",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "express": "^4.16.2",
    "file-loader": "^3.0.1",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "opn": "^5.1.0",
